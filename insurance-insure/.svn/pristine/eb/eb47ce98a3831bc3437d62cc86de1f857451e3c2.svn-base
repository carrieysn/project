header {
    width: 100%;
    height: 78px;
    border-bottom: 2px solid #0e3f89;
}
header .header-con {
    width: 1200px;
    height: 78px;
    margin: 0 auto;
}
header .header-con span {
    float: right;
    padding: 35px 10px;
    font-size: 14px;
    color: #666666;
    cursor: pointer;
}
header .header-con img {
    width: 327px;
    height: 47px;
    padding-top: 20px;
}
footer {
    width: 100%;
    border-top: 1px solid #a1a1a1;
}
footer ul {
    padding: 20px 0px;
}
footer ul {
    display: inline-block;
}
footer ul li {
    float: left;
    display: inline-block;
    padding: 0px 5px;
    border-right: 1px solid #a1a1a1;
    font-size: 12px;
    color: #a1a1a1;
}
.copyright .copyright-con {
    width: 922px;
    height: 57px;
    margin: 0 auto;
    padding-top: 10px; 
    border-top: 1px dotted #a1a1a1;
}
.copyright .copyright-con h3, .copyright .copyright-con p {
    font-size: 12px;
    line-height: 16px;
    text-align: center;
    color: #a1a1a1;
}