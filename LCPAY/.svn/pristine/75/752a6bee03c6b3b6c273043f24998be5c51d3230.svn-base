package com.cifpay.lc.gateway.input.lc;

import java.io.Serializable;

public class DeferReq implements Serializable {

	private static final long serialVersionUID = -3686160126620725082L;

	private long lcId;
	private String remark;

	public long getLcId() {
		return lcId;
	}

	public void setLcId(long lcId) {
		this.lcId = lcId;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}
}
