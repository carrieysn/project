package com.cifpay.lc.bankadapter.api.output.bank;

import com.cifpay.lc.bankadapter.api.output.AbsBusinessResult;

public class GeneralTradeResult extends AbsBusinessResult {
	private static final long serialVersionUID = 8400532039129229010L;
	/**
	 * (银行)返回交易流水
	 */
	private String rtnSerialNo;
	/**
	 * (银行)返回交易日期
	 */
	private String rtnTradeDate;
	/**
	 * (银行)返回响应码
	 */
	private String rtnCode;
	/**
	 * (银行)返回响应信息
	 */
	private String rtnMsg;

	/**
	 * (银行)返回创建冻结日期[只有冻结交易有对应值]
	 */
	private String rtnCreateFreezeDate;

	/**
	 * 系统流水号
	 */
	private Long flowId;

	public Long getFlowId() {
		return flowId;
	}

	public void setFlowId(Long flowId) {
		this.flowId = flowId;
	}

	public String getRtnSerialNo() {
		return rtnSerialNo;
	}

	public void setRtnSerialNo(String rtnSerialNo) {
		this.rtnSerialNo = rtnSerialNo;
	}

	public String getRtnTradeDate() {
		return rtnTradeDate;
	}

	public void setRtnTradeDate(String rtnTradeDate) {
		this.rtnTradeDate = rtnTradeDate;
	}

	public String getRtnCode() {
		return rtnCode;
	}

	public void setRtnCode(String rtnCode) {
		this.rtnCode = rtnCode;
	}

	public String getRtnMsg() {
		return rtnMsg;
	}

	public void setRtnMsg(String rtnMsg) {
		this.rtnMsg = rtnMsg;
	}

	public String getRtnCreateFreezeDate() {
		return rtnCreateFreezeDate;
	}

	public void setRtnCreateFreezeDate(String rtnCreateFreezeDate) {
		this.rtnCreateFreezeDate = rtnCreateFreezeDate;
	}
}
