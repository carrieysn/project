.header {
  position: relative;
  z-index: 9;
  height: 75px;
  box-shadow: 0 1px 3px #ddd; }

@media screen and (max-width: 992px) {
  .header {
    height: 50px; }
    .header .container > img {
      max-height: 40px; } }

.v-middle {
  height: 100%; }
  .v-middle:before {
    display: inline-block;
    content: '';
    height: 100%;
    vertical-align: middle; }
  .v-middle > img {
    vertical-align: middle;
    display: inline-block; }

.page-container .forminfoWrap {
  margin-bottom: 20px;
  background: #fcfcfc;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
  padding-bottom: 10px; }
  .page-container .forminfoWrap .f-info {
    padding: 10px;
    line-height: 1.8;
    font-size: 12px;
    color: #555; }
  .page-container .forminfoWrap .container {
    padding-left: 0; }
  .page-container .forminfoWrap .f-other {
    padding-right: 100px;
    text-align: right; }
    .page-container .forminfoWrap .f-other .color-blue {
      color: #0da3e2;
      font-weight: bold; }
    .page-container .forminfoWrap .f-other .p-block {
      font-size: 12px;
      margin-top: 0px;
      margin-bottom: 5px;
      color: #737373; }
  .page-container .forminfoWrap .min-show {
    display: none; }
    .page-container .forminfoWrap .min-show > a {
      padding: 0; }
  @media screen and (max-width: 992px) {
    .page-container .forminfoWrap .f-info {
      padding-bottom: 0; }
      .page-container .forminfoWrap .f-info > ul {
        margin-bottom: 0; }
    .page-container .forminfoWrap .f-other {
      padding-right: 0px;
      text-align: left;
      padding-left: 25px;
      margin-top: -10px; }
    .page-container .forminfoWrap .color-blue {
      line-height: 1; }
    .page-container .forminfoWrap .orderinfo {
      display: none; }
    .page-container .forminfoWrap .min-show {
      display: block; }
    .page-container .forminfoWrap .min-hide {
      display: none; } }

.page-container .paymentWrap .nav-tabs {
  margin-bottom: 15px; }

.page-container .paymentWrap .banklogo {
  max-width: 130px; }

.page-container .paymentWrap .bankstyle {
  display: inline-block;
  color: #666;
  padding-left: 10px; }

.page-container .paymentWrap .credit-help {
  position: relative;
  padding-left: 60px; }
  .page-container .paymentWrap .credit-help:before {
    position: absolute;
    left: 10px;
    display: block;
    content: '';
    width: 50px;
    height: 100%;
    background-repeat: no-repeat;
    background-size: 30px;
    background-position: center center;
    background-image: url("../images/unionpay.png"); }

.page-container .paymentWrap .payimg {
  max-width: 100px; }

.page-container .paymentWrap .form-inline {
  margin-bottom: 10px; }

@media screen and (min-width: 992px) {
  .page-container .paymentWrap input[type=text].form-control, .page-container .paymentWrap input[type=tel].form-control, .page-container .paymentWrap input[type=number].form-control, .page-container .paymentWrap input[type=password].form-control {
    max-width: 340px; } }

.page-container .paymentWrap input[type=text].form-control, .page-container .paymentWrap select.form-control {
  min-width: 240px; }

.page-container .paymentWrap .form-inline .form-group > label {
  margin-right: 20px; }

.page-error {
  padding: 100px 0;
  text-align: center; }
  .page-error .err-cont {
    position: relative;
    display: block;
    width: 400px;
    min-height: 128px;
    margin: 0 auto;
    padding-left: 168px; }
    .page-error .err-cont:before {
      position: absolute;
      background-image: url(../images/faild.png);
      left: 0;
      top: 0;
      content: '';
      display: inline-block;
      width: 128px;
      height: 128px; }
    .page-error .err-cont > h3 {
      display: inline-block;
      width: 400px;
      text-align: left;
      vertical-align: middle; }
    .page-error .err-cont > p {
      font-size: 12px;
      text-align: left;
      vertical-align: middle; }

@media screen and (max-width: 992px) {
  .page-error {
    padding: 50px 0; }
  .page-error .err-cont {
    padding-left: 0;
    max-width: 100%; }
    .page-error .err-cont > h3 {
      max-width: 100%;
      text-align: center; }
    .page-error .err-cont > p {
      text-align: center; }
    .page-error .err-cont:before {
      position: relative;
      width: 100%;
      background-repeat: no-repeat;
      background-position: center center; } }

.title-width {
  min-width: 5em; }

.elli, .tooltip {
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; }
