package com.cifpay.lc.gateway.output.lc;

import com.cifpay.lc.domain.security.MerchantResponseData;

public abstract class LcBaseResp extends MerchantResponseData {

	private String lcId;// 银信证ID
	private String lcStatus; // 银信证状态
	private String lcStatusDesc; // 银信证状态描述

	public String getLcId() {
		return lcId;
	}

	public void setLcId(String lcId) {
		this.lcId = lcId;
	}

	public String getLcStatus() {
		return lcStatus;
	}

	public void setLcStatus(String lcStatus) {
		this.lcStatus = lcStatus;
	}

	public String getLcStatusDesc() {
		return lcStatusDesc;
	}

	public void setLcStatusDesc(String lcStatusDesc) {
		this.lcStatusDesc = lcStatusDesc;
	}
}
