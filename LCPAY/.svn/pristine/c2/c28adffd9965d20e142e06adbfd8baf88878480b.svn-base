package com.cifpay.lc.gateway.output;

import java.io.Serializable;

/**
 * 在请求基本校验未通过的前提下，Gateway接口最终返回给客户端的异常内容。
 * 
 * 
 *
 */
public class FinalUnsignedFailureResponse implements Serializable {
	private static final long serialVersionUID = 8996012860302531114L;
	private int returnCode;
	private String returnMsg;

	public int getReturnCode() {
		return returnCode;
	}

	public void setReturnCode(int returnCode) {
		this.returnCode = returnCode;
	}

	public String getReturnMsg() {
		return returnMsg;
	}

	public void setReturnMsg(String returnMsg) {
		this.returnMsg = returnMsg;
	}

}
