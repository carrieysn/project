package com.cifpay.lc.constant.enums;

/**
 * 交易状态 处理中 0， 成功 1，失败 2，不确定 3
 */
public enum LcTranStatus {
    INPROCESS("0"), SUCCESS("1"), FAIL("2"), UNCERTAIN("3");

    private String tranStatus;

    private LcTranStatus(String tranStatus) {
        this.tranStatus = tranStatus;
    }

    public String getTranStatusStr() {
        return tranStatus;
    }
}
