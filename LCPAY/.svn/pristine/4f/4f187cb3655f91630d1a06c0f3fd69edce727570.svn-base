package com.cifpay.lc.gateway.input.batch;

import java.util.List;

public class BatchWithdrawReq {

	private String merId; // 商户号

	private List<WithdrawReq> listWithdraw;

	public List<WithdrawReq> getListWithdraw() {
		return listWithdraw;
	}

	public void setListWithdraw(List<WithdrawReq> listWithdraw) {
		this.listWithdraw = listWithdraw;
	}

	public String getMerId() {
		return merId;
	}

	public void setMerId(String merId) {
		this.merId = merId;
	}

}
