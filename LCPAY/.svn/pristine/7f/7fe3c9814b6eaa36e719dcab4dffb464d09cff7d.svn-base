package mock.merchant.bean;

public class PayNotify {

	private Long lcId;
	
	private String orderId;
	
	private String lcStatus;
	
	private String lcDesc;
	
	private String amount;
	
	private String tradeTime;
	
	private String sign;

	public Long getLcId() {
		return lcId;
	}

	public void setLcId(Long lcId) {
		this.lcId = lcId;
	}

	public String getOrderId() {
		return orderId;
	}

	public void setOrderId(String orderId) {
		this.orderId = orderId;
	}

	public String getLcStatus() {
		return lcStatus;
	}

	public void setLcStatus(String lcStatus) {
		this.lcStatus = lcStatus;
	}

	public String getLcDesc() {
		return lcDesc;
	}

	public void setLcDesc(String lcDesc) {
		this.lcDesc = lcDesc;
	}

	public String getAmount() {
		return amount;
	}

	public void setAmount(String amount) {
		this.amount = amount;
	}

	public String getTradeTime() {
		return tradeTime;
	}

	public void setTradeTime(String tradeTime) {
		this.tradeTime = tradeTime;
	}

	public String getSign() {
		return sign;
	}

	public void setSign(String sign) {
		this.sign = sign;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("PayNotify [lcId=");
		builder.append(lcId);
		builder.append(", orderId=");
		builder.append(orderId);
		builder.append(", lcStatus=");
		builder.append(lcStatus);
		builder.append(", lcDesc=");
		builder.append(lcDesc);
		builder.append(", amount=");
		builder.append(amount);
		builder.append(", tradeTime=");
		builder.append(tradeTime);
		builder.append(", sign=");
		builder.append(sign);
		builder.append("]");
		return builder.toString();
	}
	
	
}
