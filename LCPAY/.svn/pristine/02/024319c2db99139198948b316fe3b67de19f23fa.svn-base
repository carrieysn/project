package com.cifpay.lc.thirdtradeadapter.adapter.icbcbankent.exception;

import com.cifpay.lc.constant.ReturnCode;
import com.cifpay.lc.core.exception.ThirdTradePlatformAdapterException;

/**
 * NC交易返回非法的报文结果时抛出此异常
 * 
 * 
 *
 */
public class NCResponseContentInvalidException extends NCResultUncertainException {
	private static final long serialVersionUID = -4871790905864057113L;
	private static final int ERR_CODE = ReturnCode.CORE_3RD_ICBC_BANKENT_NC_TRADE_RESPONSE_INVALID;

	public NCResponseContentInvalidException(String message) {
		super(ERR_CODE, message);
	}

	public NCResponseContentInvalidException(String message, Throwable cause) {
		super(ERR_CODE, message, cause);
	}

}
