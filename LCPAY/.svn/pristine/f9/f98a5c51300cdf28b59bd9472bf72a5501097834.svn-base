package com.cifpay.lc.domain.batch;

import com.cifpay.lc.util.logging.LoggerEnum;

public class BatchOpenLcInputBean extends AbstractBatchInputBean {

    private static final long serialVersionUID = 6472622706464436631L;

    private Long batchOpenId; // 批次号
    private String merId;// 商户号

    private String remark; // 备注

    public BatchOpenLcInputBean() {
        super(LoggerEnum.Scene.BATCHOPENLC);
    }


    public Long getBatchOpenId() {
        return batchOpenId;
    }

    public void setBatchOpenId(Long batchOpenId) {
        this.batchOpenId = batchOpenId;
    }

    public String getMerId() {
        return merId;
    }

    public void setMerId(String merId) {
        this.merId = merId;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }
}
