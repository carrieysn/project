    <!--<entry key="cifpay.redis.pool.maxIdle">20</entry>-->
    <!--<entry key="cifpay.redis.pool.minIdle">5</entry>-->

    <!-- rabbitmq服务器信息
    <entry key="cifpay.rabbitmq.host">10.0.20.45</entry>
    <entry key="cifpay.rabbitmq.port">5672</entry>
    -->
    <entry key="cifpay.rabbitmq.addresses">192.168.59.188:5672</entry>
    <entry key="cifpay.rabbitmq.username">admin</entry>
