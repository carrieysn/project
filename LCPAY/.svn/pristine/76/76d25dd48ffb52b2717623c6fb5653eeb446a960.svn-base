.header {
  position: relative;
  z-index: 9;
  height: 75px;
  box-shadow: 0 1px 3px #ddd; }

@media screen and (max-width: 992px) {
  .header {
    height: 50px; }
    .header .container > img {
      max-height: 40px; } }

.v-middle {
  height: 100%; }
  .v-middle:before {
    display: inline-block;
    content: '';
    height: 100%;
    vertical-align: middle; }
  .v-middle > img {
    vertical-align: middle;
    display: inline-block; }

.page-container .forminfoWrap {
  margin-bottom: 40px;
  background: #fcfcfc;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
  padding: 10px; }
  .page-container .forminfoWrap .f-info {
    padding: 10px;
    line-height: 1.8;
    font-size: 12px;
    color: #555; }
  .page-container .forminfoWrap .f-other {
    padding-right: 100px;
    text-align: right; }
    .page-container .forminfoWrap .f-other .color-blue {
      color: #0da3e2;
      font-weight: bold; }
    .page-container .forminfoWrap .f-other .p-block {
      font-size: 12px;
      margin-top: 5px;
      margin-bottom: 10px;
      color: #737373; }
  .page-container .forminfoWrap .min-show {
    display: none; }
  @media screen and (max-width: 992px) {
    .page-container .forminfoWrap .f-info {
      padding-bottom: 0; }
      .page-container .forminfoWrap .f-info > ul {
        margin-bottom: 0; }
    .page-container .forminfoWrap .f-other {
      padding-right: 0px;
      text-align: left;
      padding-left: 25px; }
    .page-container .forminfoWrap .color-blue {
      line-height: 1; }
    .page-container .forminfoWrap .orderinfo {
      display: none; }
    .page-container .forminfoWrap .min-show {
      display: block; }
    .page-container .forminfoWrap .min-hide {
      display: none; } }

.page-container .paymentWrap .nav-tabs {
  margin-bottom: 15px; }

.page-container .paymentWrap .banklogo {
  max-width: 130px; }

.page-container .paymentWrap .bankstyle {
  display: inline-block;
  color: #666;
  padding-left: 10px; }

.page-container .paymentWrap .credit-help {
  position: relative;
  padding-left: 60px; }
  .page-container .paymentWrap .credit-help:before {
    position: absolute;
    left: 10px;
    display: block;
    content: '';
    width: 50px;
    height: 100%;
    background-repeat: no-repeat;
    background-size: 30px;
    background-position: center center;
    background-image: url("../img/unionpay.png"); }

.page-container .paymentWrap .payimg {
  max-width: 100px; }

.page-container .paymentWrap .form-inline {
  margin-bottom: 10px; }

@media screen and (min-width: 992px) {
  .page-container .paymentWrap input[type=text].form-control,  .page-container .paymentWrap input[type=password].form-control, .page-container .paymentWrap input[type=tel].form-control, .page-container .paymentWrap input[type=number].form-control {
    max-width: 340px; } }

.page-container .paymentWrap input[type=text].form-control,.page-container .paymentWrap input[type=password].form-control, .page-container .paymentWrap select.form-control {
  min-width: 240px; }

.page-container .paymentWrap .form-inline .form-group > label {
  margin-right: 20px; }

.title-width{
  min-width: 5em;
}