package com.cifpay.lc.std.domain.kernel;

import java.io.Serializable;

public abstract class KernelBaseOutputBean implements Serializable {

	private static final long serialVersionUID = -1843688743170080960L;

	private Long lcId; 			// 银信证ID
	private String lcStatus; 	// 银信证状态
	private String lcStatusDesc; // 银信证状态描述

	public Long getLcId() {
		return lcId;
	}

	public void setLcId(Long lcId) {
		this.lcId = lcId;
	}

	public String getLcStatus() {
		return lcStatus;
	}

	public void setLcStatus(String lcStatus) {
		this.lcStatus = lcStatus;
	}

	public String getLcStatusDesc() {
		return lcStatusDesc;
	}

	public void setLcStatusDesc(String lcStatusDesc) {
		this.lcStatusDesc = lcStatusDesc;
	}
}
