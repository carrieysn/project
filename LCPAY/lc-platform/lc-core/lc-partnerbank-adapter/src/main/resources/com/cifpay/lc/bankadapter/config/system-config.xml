<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>银信证合作银行交易适配器应用所需的配置信息</comment>

	<!-- Dubbo注册中心zookeeper地址 -->
	<entry key="dubbo.registry.zookeeper.address">127.0.0.1:2181</entry>
	<entry key="dubbo.registry.cacheFilePathPrefix">c:/app/cifpaylc/partnerbank-adapter/data/dubbo.registry.cache</entry>
	
	<entry key="icbc.freeze.url">http://127.0.0.1:8081/ICBCMock/Freeze</entry>
	<entry key="icbc.unfreeze.url">http://127.0.0.1:8081/ICBCMock/Unfreeze</entry>
	<entry key="icbc.pay.url">http://127.0.0.1:8081/ICBCMock/Pay</entry>
	<entry key="icbc.sync.url">http://127.0.0.1:8081/ICBCMock/Sync</entry>
	
	<!-- Redis服务器信息  (非集群环境使用下面的配置)  -->
    <entry key="cifpay.redis.host">localhost</entry>
    <entry key="cifpay.redis.port">6379</entry>
    <entry key="cifpay.redis.cluster.nodes"></entry>
	<entry key="cifpay.redis.timeout">5000</entry>
	<entry key="cifpay.redis.pool.maxActive">100</entry>
	<entry key="cifpay.redis.pool.maxWait">3000</entry>
	<entry key="cifpay.redis.pool.maxIdle">20</entry>
	<entry key="cifpay.redis.pool.minIdle">5</entry>
</properties>