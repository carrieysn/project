package com.cifpay.insurance.param.policy;

import java.io.Serializable;

/**
 * 
 * @author 张均锋
 *
 */
public class GetPolicyListResult implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	/** 保单流水号 **/
	private String vendorId;
	/** 保单流水号 **/
	private Long policyId;
	/** 保单号 **/
	private String policyNo;
	/** 险种名称 **/
	private String product;
	/** 投保人信息 **/
	private PolicyHolderInfo policyHolderInfo;
	/** 被保人 **/
	private String insuredName;
	/** 支付方式（1-银信证） **/
	private Integer payMode;
	/** 预缴保费（分） **/
	private Long initPremium;
	/** 当前保费（分） **/
	private Long premium;
	/** 杠杆比例 **/
	private Integer gearing;
	/** 投保日期 **/
	private Long insureDate;
	/** 审核日期 **/
	private Long auditTime;
	/** 当前保额（分） **/
	private Long insuredAmount;
	/** 状态（0-未生效；5-正常（审核通过）；6-预警；7-警报） **/
	private Integer status;
	/** 保险人信息 **/
	private InsurerInfo insurerInfo;

	public String getVendorId() {
		return vendorId;
	}

	public void setVendorId(String vendorId) {
		this.vendorId = vendorId;
	}

	public Long getPolicyId() {
		return policyId;
	}

	public void setPolicyId(Long policyId) {
		this.policyId = policyId;
	}

	public String getPolicyNo() {
		return policyNo;
	}

	public void setPolicyNo(String policyNo) {
		this.policyNo = policyNo;
	}

	public String getProduct() {
		return product;
	}

	public void setProduct(String product) {
		this.product = product;
	}

	public PolicyHolderInfo getPolicyHolderInfo() {
		return policyHolderInfo;
	}

	public void setPolicyHolderInfo(PolicyHolderInfo policyHolderInfo) {
		this.policyHolderInfo = policyHolderInfo;
	}

	public String getInsuredName() {
		return insuredName;
	}

	public void setInsuredName(String insuredName) {
		this.insuredName = insuredName;
	}

	public Integer getPayMode() {
		return payMode;
	}

	public void setPayMode(Integer payMode) {
		this.payMode = payMode;
	}

	public Long getInitPremium() {
		return initPremium;
	}

	public void setInitPremium(Long initPremium) {
		this.initPremium = initPremium;
	}

	public Long getPremium() {
		return premium;
	}

	public void setPremium(Long premium) {
		this.premium = premium;
	}

	public Integer getGearing() {
		return gearing;
	}

	public void setGearing(Integer gearing) {
		this.gearing = gearing;
	}

	public Long getInsureDate() {
		return insureDate;
	}

	public void setInsureDate(Long insureDate) {
		this.insureDate = insureDate;
	}

	public Long getAuditTime() {
		return auditTime;
	}

	public void setAuditTime(Long auditTime) {
		this.auditTime = auditTime;
	}

	public Long getInsuredAmount() {
		return insuredAmount;
	}

	public void setInsuredAmount(Long insuredAmount) {
		this.insuredAmount = insuredAmount;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public InsurerInfo getInsurerInfo() {
		return insurerInfo;
	}

	public void setInsurerInfo(InsurerInfo insurerInfo) {
		this.insurerInfo = insurerInfo;
	}

}
