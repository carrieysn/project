package com.cifpay.insurance.param.policy;

import java.io.Serializable;

import com.cifpay.insurance.param.PageInfo;

/**
 * 查询保单列表。
 * 
 * @author 张均锋
 *
 */
public class GetPolicyListInfo extends PageInfo implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private String vendorId;

	private String policyNo;
	/** 投保人 **/
	private String holderName;
	/** 证件号 **/
	private String idNo;
	/** 投保起始日期 **/
	private String insureDateFrom;
	/** 投保截止日期 **/
	private String insureDateTo;
	/** 审核起始日期 **/
	private String auditTimeFrom;
	/** 审核截止日期 **/
	private String auditTimeTo;
	/** 保单状态（0-待审核；10-已审核） **/
	private Integer status;

	public String getVendorId() {
		return vendorId;
	}

	public void setVendorId(String vendorId) {
		this.vendorId = vendorId;
	}

	public String getPolicyNo() {
		return policyNo;
	}

	public void setPolicyNo(String policyNo) {
		this.policyNo = policyNo;
	}

	public String getHolderName() {
		return holderName;
	}

	public void setHolderName(String holderName) {
		this.holderName = holderName;
	}

	public String getIdNo() {
		return idNo;
	}

	public void setIdNo(String idNo) {
		this.idNo = idNo;
	}

	public String getInsureDateFrom() {
		return insureDateFrom;
	}

	public void setInsureDateFrom(String insureDateFrom) {
		this.insureDateFrom = insureDateFrom;
	}

	public String getInsureDateTo() {
		return insureDateTo;
	}

	public void setInsureDateTo(String insureDateTo) {
		this.insureDateTo = insureDateTo;
	}

	public String getAuditTimeFrom() {
		return auditTimeFrom;
	}

	public void setAuditTimeFrom(String auditTimeFrom) {
		this.auditTimeFrom = auditTimeFrom;
	}

	public String getAuditTimeTo() {
		return auditTimeTo;
	}

	public void setAuditTimeTo(String auditTimeTo) {
		this.auditTimeTo = auditTimeTo;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

}
