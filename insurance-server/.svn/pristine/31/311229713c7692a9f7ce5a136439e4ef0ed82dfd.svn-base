package com.cifpay.insurance.bean;

import java.io.Serializable;

/**
 * 商户今日新增汇总
 * 
 * @author 张均锋
 *
 */
public class TodayAddedCertBean implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String vendorId;

	private int todayAddedCertCount;

	private long amount;

	public String getVendorId() {
		return vendorId;
	}

	public void setVendorId(String vendorId) {
		this.vendorId = vendorId;
	}

	public int getTodayAddedCertCount() {
		return todayAddedCertCount;
	}

	public void setTodayAddedCertCount(int todayAddedCertCount) {
		this.todayAddedCertCount = todayAddedCertCount;
	}

	public long getAmount() {
		return amount;
	}

	public void setAmount(long amount) {
		this.amount = amount;
	}

}
