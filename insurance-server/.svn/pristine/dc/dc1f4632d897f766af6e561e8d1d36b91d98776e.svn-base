package com.cifpay.insurance.param.cert;

import java.io.Serializable;

/**
 * 保险证拒绝退款
 * 
 * @author 张均锋
 *
 */
public class InsuranceCertRefundRefuseInfo implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	/** 保险证号 **/
	private String insuranceCertNo;
    /** 拒绝原因 **/
	private String refuseRefundCause;

	public String getInsuranceCertNo() {
		return insuranceCertNo;
	}

	public void setInsuranceCertNo(String insuranceCertNo) {
		this.insuranceCertNo = insuranceCertNo;
	}

	public String getRefuseRefundCause() {
		return refuseRefundCause;
	}

	public void setRefuseRefundCause(String refuseRefundCause) {
		this.refuseRefundCause = refuseRefundCause;
	}

}
